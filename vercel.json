{
    "version": 2,
    "builds": [
        {
            "src": "functions/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/swagger-ui/(.*)",
            "dest": "/node_modules/swagger-ui-dist/$1"
        },
        {
            "src": "/api-docs",
            "dest": "/api-docs"
        },
        {
            "src": "/(.*)",
            "dest": "functions/index.js"
        }
    ]
}